{
  "name": "e2e-tests",
  "version": "1.0.0",
  "description": "E2E tests for the application. Written with Nightwatch and Gherkin.",
  "scripts": {
    "lint": "yarn prettier --write .",
    "test:e2e": "cucumber-js --require cucumber.conf.js --require tests/acceptance/stepDefinitions",
    "test:e2e:ci": "cucumber-js --retry 1 --require cucumber.conf.js --require tests/acceptance/stepDefinitions",
    "test:ci": "yarn test:e2e:ci tests/acceptance/features -t 'not @skip'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiranparajuli589/Moving-Todo.git"
  },
  "author": "Kiran Parajuli",
  "bugs": {
    "url": "https://github.com/kiranparajuli589/Moving-Todo/issues"
  },
  "homepage": "https://github.com/kiranparajuli589/Moving-Todo#readme",
  "dependencies": {
    "@cucumber/cucumber": "^8.0.0-rc.1",
    "axios": "^0.24.0",
    "chromedriver": "^96.0.0",
    "geckodriver": "^2.0.4",
    "iedriver": "^3.150.1",
    "nightwatch": "^1.3.4",
    "nightwatch-api": "^3.0.1",
    "selenium-server": "^3.141.59"
  },
  "devDependencies": {
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "prettier": "^2.5.1"
  }
}
